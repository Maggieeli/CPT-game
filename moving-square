x = 60
y = 60

global x
global y

def setup():
    size(640,480)
    background(0)
    
def draw():
    background(0)
    rect(x,y,60,60)
    
def keyPressed():
    global x
    global y
    if (key == CODED):
        if (keyCode == UP):
            y -= 10
        elif (keyCode == DOWN):
            y += 10
        elif (keyCode == LEFT):
            x -= 10
        elif (keyCode == RIGHT):
            x += 10
